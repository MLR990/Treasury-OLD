@model Treasury.Models.CofferModel

@{
    ViewData["Title"] = "Vendors";
}
<h2>@ViewData["Title"]</h2>
<h3>@ViewData["Message"]</h3>
<head>

</head>

<p>Use this area to provide additional information.</p>
Name: <input type="text" id="cofferName" /><br /><br />
Description: <input type="text" id="cofferDescription" /><br /><br />
Amount: <input type="text" id="cofferAmount" /><br /><br />
Order: <input type="text" id="cofferOrder" /><br /><br />
Coffer Type:
<select id="cofferType">
    <option>Monthly</option>
    <option>One Time</option>
    <option>Annual</option>
</select><br /><br />
Budget Month
<select id="cofferMonth">
    <option>January</option>
    <option>Febuary</option>
    <option>March</option>
    <option>April</option>
    <option>May</option>
    <option>June</option>
    <option>July</option>
    <option>August</option>
    <option>September</option>
    <option>October</option>
    <option>November</option>
    <option>December</option>
</select>

<br /><br />
Necessary: <input type="checkbox" id="cofferNecessary" value="true" name="necessary" /><br /><br />
Budget: @Html.DropDownListFor(model => model.BudgetId, new SelectList(Model.Budgets, "Id", "Name"))<br /><br />
<input type="button" id="addCoffer" value="Add Coffer" />
